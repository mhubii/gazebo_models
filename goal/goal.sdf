<?xml version="1.0"?>
<sdf version="1.6">

<!-- Goal. -->
<model name="goal">
  <static>true</static>

  <link name="goal">
    <pose>0 0 0.4 0 0 0</pose>
  
    <collision name="goal_collision">
      <geometry>
        <sphere>
          <radius>0.4</radius>
        </sphere>
      </geometry>
    </collision>

    <visual name="visual">
      <geometry>
        <sphere>
          <radius>0.4</radius>
        </sphere>
      </geometry>
      <material>
        <script>
          <uri>model://goal/materials/scripts</uri>
          <uri>model://goal/materials/textures</uri>
          <name>goal</name>
        </script>
      </material>
    </visual>
  </link>
</model>
</sdf>
